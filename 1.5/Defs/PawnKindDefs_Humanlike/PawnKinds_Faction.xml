<Defs>

	<!--====================== Faction Pawns for Logann Map Factions ======================-->
	
	<!-- a lot of these edits and tags weren't available in the original mod so there's a lot more control available here, OG is pretty much 'here is this pawn, it exists'.-->
	<!-- kept it as original as possible-->

	<!--====================== Logann Colonist ======================-->
	
	<!-- Logann Colonist - base faction pawn -->
	
	<PawnKindDef>
		<defName>LogannColonistPawn</defName>
		<label>logann colonist</label>
		<race>Human</race>
		<defaultFactionType>LogannMilitaryState</defaultFactionType>
		<isFighter>True</isFighter>
		<maxGenerationAge>85</maxGenerationAge>
		<combatPower>25</combatPower> <!--not sure this is properly scaled, for reference a sanguophage is 130-->
		<invNutrition>2.55</invNutrition>
		<chemicalAddictionChance>0.06</chemicalAddictionChance>
		<backstoryCryptosleepCommonality>0.8</backstoryCryptosleepCommonality>
		<initialWillRange>3~5</initialWillRange>
		<initialResistanceRange>20~42</initialResistanceRange>
		<acceptArrestChanceFactor>1.5</acceptArrestChanceFactor>
		<itemQuality>Good</itemQuality>
		<backstoryFiltersOverride>
			<li>
				<categoriesChildhood>
					<li>LogannBackstory</li>
				</categoriesChildhood>
				<categoriesAdulthood>
					<li>LogannBackstory</li>
				</categoriesAdulthood>
			</li>
		</backstoryFiltersOverride>
		<weaponMoney>1400~2500</weaponMoney> <!-- i have a feeling this might not scale well but its the original value -->
		<weaponTags>
			<li>LogannWep</li>
		</weaponTags>
		<!-- can modify to ensure armor or other spawn, otherwise it'll random select out of the next tabs  -->
		<!-- 
		<apparelRequired> 
			<li>Apparel_CollarShirt</li>
			<li>Apparel_Pants</li>
		</apparelRequired>
		-->
		<apparelMoney>300~900</apparelMoney> <!-- this also doesn't look like it'll scale well -->
		<apparelTags>
			<li>Outlander</li>
			<li>Military</li>
			<li>IndustrialBasic</li>
			<li>IndustrialAdvanced</li>
			<li>IndustrialMilitaryBasic</li>
			<li>IndustrialMilitaryAdvanced</li>
			<li>SpacerMilitary</li>
		</apparelTags>
		<apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
		<!--we can force a color if we want to go with an 'army' look, and modify stuff vals -->
		<!--
		<specificApparelRequirements Inherit="False">
			<li>
				<bodyPartGroup>FullHead</bodyPartGroup>
				<color>(220,220,220)</color>
			</li>
			<li>
				<bodyPartGroup>Torso</bodyPartGroup>
				<apparelLayer>Shell</apparelLayer>
				<color>(220,220,220)</color>
			</li>
			<li>
				<bodyPartGroup>Legs</bodyPartGroup>
				<apparelLayer>OnSkin</apparelLayer>
				<color>(220,220,220)</color>
				<stuff>Synthread</stuff> 
			</li>
		</specificApparelRequirements>
		-->
		<techHediffsMoney>200~300</techHediffsMoney> <!-- if spawned with aug, make it this expensive, might need to up this -->
		<techHediffsTags>
			<li>Simple</li>
		</techHediffsTags>
		<techHediffsChance>0.51</techHediffsChance> <!--can increase spawn chance here if wanted -->
		<useFactionXenotypes>false</useFactionXenotypes> <!-- override in next section-->
		<xenotypeSet>
			<xenotypeChances>
				<Logann MayRequire="Ludeon.RimWorld.Biotech">1</Logann>
			</xenotypeChances>
		</xenotypeSet>
		<!--can force certain traits so the don't spawn as like, incapable of violence -->
		<requiredWorkTags>
			<li>Violent</li>
		</requiredWorkTags>
		<gearHealthRange>1</gearHealthRange>
		<!--this section controls starting pawn boosts to skills naturally, so we can up them for certain pawntypes, also keep in mind their backstory will further modify-->
		<skills>
			<li>
				<skill>Shooting</skill>
				<range>2~8</range>
			</li>
			<li>
				<skill>Melee</skill>
				<range>2~8</range>
			</li>
		</skills>
		<!-- can remove certain traits, I think this one should be covered by culture but I think by default the Logann are pretty cool with body mods...remind me to add in the bionic tail at some point -->
		<disallowedTraits>
			<li>BodyPurist</li>
		</disallowedTraits>
		<inventoryOptions>
			<skipChance>0.75</skipChance>
			<subOptionsChooseOne>
				<li>
					<thingDef>Silver</thingDef>
					<choiceChance>90</choiceChance>
					<countRange>25~70</countRange>
				</li>
				<li>
					<thingDef>Silver</thingDef>
					<choiceChance>10</choiceChance>
					<countRange>700~1000</countRange>
				</li>
			</subOptionsChooseOne>
		</inventoryOptions>
	</PawnKindDef>

